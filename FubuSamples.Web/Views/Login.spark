<viewdata model="FubuSamples.Web.Handlers.FormModel" />
<html>
	<head>
		<title>Please log in, dear user</title>
	</head>
	<body>
		${this.FormFor(new FubuSamples.Web.Handlers.FormModel())}
			<p>
				<label for="Login">Login: ${this.InputFor(m => m.Login)}</label>				
			</p>
			<p>
				<label for="Password">Password: ${this.InputFor(m => m.Password)}</label>
			</p>

			<input type="submit" value="Войти" />	
	</body>
</html>